{
    "name": "picoframe/picoframe",
    "desvription": "Un micro framework PHP simple y potente.",
    "version": "1.0.0",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "bramus/router": "^1.6",
        "illuminate/database": "^12.24",
        "firebase/php-jwt": "^6.11",
        "rakit/validation": "^1.4",
        "twig/twig": "^3.21",
        "php-di/php-di": "^7.0",
        "filp/whoops": "^2.18",
        "monolog/monolog": "^3.9"
    },
    "scripts": {
        "post-create-project-cmd": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "php pico key:generate"
        ]
    }

}
